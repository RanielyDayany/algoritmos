Algoritmo "parede"

Var
   Al,L,A,T:Real

Inicio
   Escreva ("Qual a altura da parede?")
   Leia (Al)
   Escreva ("Qual a largura da parede?")
   Leia (L)
   A <- AL * L
   T <- A/2
   EscrevaL ("A área a ser pintada é de ",A,"m2")
   Escreva ("Será necessário ",T," baldes de tinta para pintar toda a parede")
Fimalgoritmo