Algoritmo "creuza03"

// Descrição:Calcula o valor do imposto sobre o produto.

Var 
          preco,imposto:Real

Inicio
          Escreva("Qual o valor do produto?US$")
          Leia (preco)
          imposto<-(preco*60)/100
          Escreva("O valor do imposto será de US$",imposto:5:2)
Fimalgoritmo