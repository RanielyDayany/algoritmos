Algoritmo "repita01"

Var
   c:inteiro

Inicio
   //Contagem ate 30 com Repita
   c <- 0
   EscrevaL("          CONTAGEM          ")
   EscrevaL("----------------------------")
   Repita
      Escreva(c," ")
      c <- c + 3
   Ate (c > 30)
   EscrevaL("")
   EscrevaL("Contagem concluida!")
Fimalgoritmo