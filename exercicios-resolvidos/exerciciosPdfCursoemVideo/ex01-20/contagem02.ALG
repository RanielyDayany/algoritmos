Algoritmo "contagem02"

Var
   n:inteiro
   
Inicio
   n <- 10
   EscrevaL("------------------------")
   EscrevaL("|     VAMOS CONTAR     |")
   EscrevaL("------------------------")
   Enquanto (n > 0) faca
      Escreva(n)
      n <- n - 1
   FimEnquanto
   EscrevaL("")
   Escreva("  ACABOU!")
Fimalgoritmo