Algoritmo "contagem01"

Var
   n:inteiro

Inicio
   n<-0
   EscrevaL("------------------------")
   EscrevaL("|     VAMOS CONTAR!    |")
   EscrevaL("------------------------")
   Enquanto (n <=10) faca
      n <- n+1
      Escreva (n)
   FimEnquanto
   EscrevaL("")
   Escreva(" ACABOU!")
Fimalgoritmo